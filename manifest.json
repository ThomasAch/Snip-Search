{
  "manifest_version": 3,
  "name": "Snip&Search",
  "version": "1.0",
  "description": "A extension that snips a screenshot and then reverse image searches it",
  "permissions": ["tabs", "contextMenus", "activeTab", "scripting"],
  "icons": {
    "128": "SnipSearchIcon1v2.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "SnipSearchIcon1v2.png",
    "default_title": "Snip&Search"
  },
  "background": {
    "service_worker": "background.js"
  }
}
